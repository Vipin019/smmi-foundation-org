<%- include("partials/header"); -%>
<h1>Compose</h1>
<form  action="/compose" method="post">
    <div class="form-group">
      <label for="inputTitle">Title</label>
      <input type="text" name="newPostTitle" class="form-control" id="inputTitle" placeholder="Enter title">
    </div>
    <div class="form-group">
      <label for="inputPost">Password</label>
      <textarea name="newPostContent" id="inputPost" class="form-control"></textarea>
    </div>
    <button  type="submit" name="button" class="btn btn-primary">Publish</button>
  </form>
<%- include("partials/footer"); -%>